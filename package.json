{
  "name": "meos-api-helper",
  "version": "0.1.1",
  "description": "Package to get JSON object from the MEOS information server to make it faster and more accesible to make applications against MEOS.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/malpou/meos-api-helper.git"
  },
  "author": "Malthe Poulsen",
  "keywords": [
    "Orienteering",
    "MEOS"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/malpou/meos-api-helper/issues"
  },
  "homepage": "https://github.com/malpou/meos-api-helper#readme",
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "jest": "^26.4.2",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.3"
  },
  "files": [
    "lib/**/*"
  ]
}
